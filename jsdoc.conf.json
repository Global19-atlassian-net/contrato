{
  "plugins": [],
  "opts": {
    "access": "public",
    "destination": "./docs",
    "pedantic": true,
    "readme": "README.md",
    "template": "./node_modules/ink-docstrap/template"
  },
  "recurseDepth": 10,
  "source": {
    "include": [ "lib" ],
    "exclude": [ "test" ],
    "includePattern": ".+\\.js?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [ "jsdoc", "closure" ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}

